<!-- Template for Burger Page -->
<h1>Eat A Burger</h1>
  
{{#each burgers}}
<pre>
{{id}}  {{burger_name}} {{devoured}} {{date_created}}
</pre>
{{/each}}

<form action="/create" method="POST">
  <div class="form-group">
    <label for="exampleInput1">Burger Order</label>
    <input type="text" class="form-control" id="exampleInput1" placeholder="Order Your Burger">
  </div>
  

  <button type="submit" class="btn btn-default">Submit</button>
</form>

